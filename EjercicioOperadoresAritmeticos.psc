Algoritmo EjercicioOperadoresAritmeticos
	Imprimir 'Proporciona el primer valor : '
	Leer valor1
	Imprimir  'Proporciona el segundo valor : '
	Leer valor2
	Imprimir 'Suma : ', valor1 + valor2
	Imprimir 'Resta : ', valor1 - valor2
	Imprimir 'Multiplicación : ', valor1 * valor2
	Imprimir 'División : ', valor1 / valor2
	Imprimir 'Modulo residuo division : ', valor1 MOD valor2
	Imprimir 'Potencia : ', valor1 ^ valor2
FinAlgoritmo
