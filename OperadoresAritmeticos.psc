Algoritmo OperadoresAritmeticos
	//Operadores Aritmeticos
	//Suma +
	a = 3 + 4
	Imprimir 'Suma: ',  a
	
	//Resta - 
	b = 6 - 2
	Imprimir 'Resta; ', b
	
	//Multiplicación
	c = a * 2
	Imprimir 'Multiplicacion: ', c
	
	//Division
	d = b / c
	Imprimir 'División: ', d
	
	//Potencia (Flecha hacia arriba)
	e = a ^ 2
	Imprimir  'Potencia: ', e
	
	//Modulo o residuo de la división
	f = 9 % 3
	Imprimir 'Modulo: ', f
	
FinAlgoritmo
